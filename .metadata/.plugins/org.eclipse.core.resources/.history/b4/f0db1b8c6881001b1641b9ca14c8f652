package oop0310;

public class Test05_while {
public static void main(String[] args) {
	int a = 1;
	while (a<4) {
		System.out.println(a+ " JAVA");
		a++;
	}
	
	int b=1;
	do {
		System.out.println(b+ " ÆÄÀÌ½ã");
		b++;
	}while(b<4);
	
	int c=1;
	do {
		
		System.out.println(c+" HTML");
		c++;
	}while(c<=0);
}
}
