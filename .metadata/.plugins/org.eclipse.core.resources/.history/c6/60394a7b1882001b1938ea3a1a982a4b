package oop0311;

public class Test04_quiz {

	public static void main(String[] args) {
		int [] num = {7,8,-9,5,6};
		int size = num.length; //5
		
		//1) 배열요소의 전체 합
		int result = 0;
		for(int i =0;i<size; i++) {
			result +=num[i];
			System.out.println("전체 합 : "+result);
		}
		//2) 음수의 갯수
		//3) 최대값, 최소값
		//4)num[4]요소의 등수를 구하시오
		

	}

}
