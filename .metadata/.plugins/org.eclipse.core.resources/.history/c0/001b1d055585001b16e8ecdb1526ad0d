package oop0315;

public class Jumsu {
		private int no;
		private String name;
		private boolean[] problem;
		private int score;
		private int rank;
		
		public Jumsu(){};
		public Jumsu(int no, String name) {
			this.no=no;
			this.name=name;}
		
		public void calc(boolean[] problem) {
			for(boolean p : problem) {
				if(p) System.out.println("O");
				else System.out.println("X");
			}
		}
		
}
