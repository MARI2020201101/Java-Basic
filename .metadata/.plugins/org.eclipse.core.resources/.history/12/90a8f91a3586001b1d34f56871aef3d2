package oop0316;
class AA{
	private void zero() {}
	public void one() {
		System.out.println("AA.one....");
	}
	public void two() {
		System.out.println("AA.two....");
	}
}
class BB extends AA{
	public void three() {
		System.out.println("BB.three....");
	}
}
class CC extends BB{
	public void four() {
		System.out.println("CC.four...");
	}
}



public class Test09_sangsok {

	public static void main(String[] args) {
		// extends: 상속. 부모자식관계를 만든다 . 자바는 다중상속불가
	}

}
