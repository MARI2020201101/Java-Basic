package oop0315;

public class Jumsu {
		private int no;
		private String name;
		private boolean[] problem = new boolean[5];
		private char[] ox = new char[5];
		private int score;
		private int rank;
		
		public Jumsu(){};
		public Jumsu(int no, String name) {
			this.no=no;
			this.name=name;}
		
		public void calc(boolean[] problem) {
			int score =0;
			for(int i=0; 0<problem.length; i++) {
				if(problem[i]) {
					ox[i] = 'O'; 
					score+=20;
				}else {ox[i] = 'X';}
			}this.score = score;
			
			System.out.print(score);
		}
		
		public void calcRank() {
			
		}
		
}
