package oop0311;

public class Test02_array {

	public static void main(String[] args) {
		int[][] kor = new int[2][3];
		kor[0][0] =10;
		kor[0][1] =20;
		kor[0][2] =30;
		kor[1][0] =40;
		kor[1][1] =50;
		kor[1][2] =70;
		
		System.out.println(kor.length);
		System.out.println(kor[0].length);
		System.out.println(kor[1].length);
		

	}

}
